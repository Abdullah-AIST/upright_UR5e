<?xml version="1.0" encoding="utf-8"?>
<robot name="Thing">
  <material name="dark_grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="light_grey">
    <color rgba="0.4 0.4 0.4 1.0"/>
  </material>
  <material name="red">
    <color rgba="0.8 0.0 0.0 1.0"/>
  </material>
  <material name="white">
    <color rgba="0.9 0.9 0.9 1.0"/>
  </material>
  <material name="yellow">
    <color rgba="0.8 0.8 0.0 1.0"/>
  </material>
  <material name="black">
    <color rgba="0.15 0.15 0.15 1.0"/>
  </material>
  <material name="collision_color">
    <color rgba="0.0 1.0 0.0 0.3"/>
  </material>

  <link name="world_link">
    <inertial>
      <mass value="0"/>
      <inertia ixx="0.2125" ixy="-0.005" ixz="0.0225" iyy="0.205" iyz="0.045" izz="0.0125"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </inertial>
  </link>

	<link name="x_prismatic_link">
	 <inertial>
     <mass value="0.01"/>
     <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
     <origin rpy="0 0 0" xyz="0 0 0"/>
    </inertial>
  </link>

	<joint name="x_to_world_joint" type="prismatic">
    <parent link="world_link"/>
    <child link="x_prismatic_link"/>
    <axis xyz="1 0 0"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <limit effort="0.0" velocity="1000.0"/>
  </joint>

	<link name="y_prismatic_link">
	 <inertial>
     <mass value="0.01"/>
     <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
     <origin rpy="0 0 0" xyz="0 0 0"/>
    </inertial>
  </link>

	<joint name="y_to_x_joint" type="prismatic">
    <parent link="x_prismatic_link"/>
    <child link="y_prismatic_link"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <limit effort="0.0" velocity="1000.0"/>
  </joint>

  <!-- Note the z-offset to raise the base off the ground, to avoid collision issues. Without this, there are vibrations and other artifacts from resolving the ground collision that result in significant error in the balanced object. -->
	<joint name="base_to_y_joint" type="continuous">
    <parent link="y_prismatic_link"/>
    <child link="base_link"/>
    <axis xyz="0 0 1"/>
    <origin rpy="0 0 0" xyz="0 0 0.01"/>
  </joint>

  <link name="base_link">
	 <inertial>
     <mass value="0.01"/>
     <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
     <origin rpy="0 0 0" xyz="0 0 0"/>
    </inertial>
  </link>
  <joint name="base_link_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="base_link"/>
    <child link="chassis_link"/>
  </joint>
