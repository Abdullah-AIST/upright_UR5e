endEffector
{
  name "thing_tool" ; name defined in the URDF

  ; end effector quadratic penalty scaling
  muPosition  10.0
  muOrientation  5.0
}

finalEndEffector
{
  name "thing_tool"
  muPosition  10.0
  muOrientation  5.0
}

