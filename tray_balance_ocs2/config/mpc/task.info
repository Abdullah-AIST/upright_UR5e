  (6,0) -0.7854  ; EL_ROT
  (7,0) -1.5708  ; WR_FLE
  (8,0)  1.5708  ; WR_ROT

  (9,0)  0.0  ; velocity x
  (10,0)  0.0  ; velocity y
  (11,0)  0.0  ; angular velocity
  (12,0)  0.0  ; arm 1
  (13,0)  0.0  ; arm 2
  (14,0)  0.0  ; arm 3
  (15,0)  0.0  ; arm 4
  (16,0)  0.0  ; arm 5
  (17,0)  0.0  ; arm 6
